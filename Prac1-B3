  {
   "cell_type": "code",
   "execution_count": 17,
   "id": "2e843eca",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter a string : \n",
      "4B3A\n",
      "BBBBAAA\n"
     ]
    }
   ],
   "source": [
    "class Solution:\n",
    "   def solve(str, s):\n",
    "      output = \"\"\n",
    "      num=\"\"\n",
    "      for i in s:\n",
    "         if i.isalpha():\n",
    "            output+=i*int(num)\n",
    "            num=\"\"\n",
    "         else:\n",
    "            num+=i\n",
    "      return output\n",
    "print(\"Enter a string : \")\n",
    "str = input()\n",
    "ob = Solution() \n",
    "print(ob.solve(str))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 28,
   "id": "5ad1d1e1",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[4, 5]\n"
     ]
    }
   ],
